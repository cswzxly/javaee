<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
    "http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
	<package name="FrontZlContentsAction" namespace="/web" extends="base_default">
		<!--  -->
		<action name="frontZlContentsRight" class="com.web.action.FrontZlContentsAction" method="frontZlContentsRight">
			<interceptor-ref name="initFrontCatalogStack" />
			<result name="success">/view/web/frontArea/front_contents_right.jsp</result>
			<result name="error">/view/web/error.jsp</result>
		</action>
		
		<action name="frontZlContentsDetail" class="com.web.action.FrontZlContentsAction" method="frontZlContentsDetail">
			<interceptor-ref name="initFrontCatalogStack" />
			<result name="success">/view/web/frontArea/front_contents_detail.jsp</result>
			<result name="error">/view/web/error.jsp</result>
		</action>
		
	</package>
</struts>